{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@src/*": ["./src/*"],
      "@schema/*": ["./schema/*"],
      "@assets/*": ["./assets/*"],
      // src > *
      "@components/*": ["./src/components/*"],
      "@helpers/*": ["./src/helpers/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@routes/*": ["./src/routes/*"],
      "@styles/*": ["./src/styles/*"],
      // schema > *
      "@constants/*": ["./schema/contants/*"],
      "@models/*": ["./schema/models/*"],
      // assets > *
      "@icons/*": ["./assets/icons/*"]
    }
  }
}
